{
    "apiVersion": "machinelearning.seldon.io/v1alpha1",
    "kind": "SeldonDeployment",
    "metadata": {
	"labels": {
	    "app": "seldon"
	},
	"name": "cartpole-rl-abtest"
    },
    "spec": {
	"annotations": {
	    "project_name": "Cartpole RL ABTest",
	    "deployment_version": "v1"
	},
	"name": "cartpole-rl-abtest",
	"oauth_key": "oauth-key",
	"oauth_secret": "oauth-secret",
	"predictors": [
	    {
		"componentSpec": {
		    "spec": {
			"containers": [
			    {
				"image": "hypnosapos/cartpolerlremoteagent:1ef114d",
				"imagePullPolicy": "IfNotPresent",
				"name": "cartpole-rl-a",
				"resources": {
				    "requests": {
					"memory": "1Mi"
				    }
				}
			    },
			    {
				"image": "hypnosapos/cartpolerlremoteagent:1ef114d",
				"imagePullPolicy": "IfNotPresent",
				"name": "cartpole-rl-b",
				"resources": {
				    "requests": {
					"memory": "1Mi"
				    }
				}
			    }
			],
			"terminationGracePeriodSeconds": 20
		    }
		},
		"name": "cartpole-rl-abtest-predictor",
		"replicas": 1,
		"annotations": {
		    "predictor_version": "v1"
		},
		"graph": {
		    "name": "cartpole-rl-abtest",
		    "endpoint":{},
		    "implementation":"RANDOM_ABTEST",
		    "parameters": [
			{
			    "name":"ratioA",
			    "value":"0.7",
			    "type":"FLOAT"
			}
		    ],
		    "children": [
			{
			    "name": "cartpole-rl-a",
			    "endpoint":{
				"type":"REST"
			    },
			    "type":"MODEL",
			    "children":[]
			},
			{
			    "name": "cartpole-rl-b",
			    "endpoint":{
				"type":"REST"
			    },
			    "type":"MODEL",
			    "children":[]
			}
		    ]
		}
	    }
	]
    }
}



